//🐱‍🏍Calcule a soma dos números de 1 a 100.
programa
{
	
	funcao inicio()
	{
		inteiro contador = 1
		inteiro soma = 0

		enquanto(contador <= 100){
			soma = soma + contador
			escreva(soma,"\n")
			contador++
		}
	}
}
